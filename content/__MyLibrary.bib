@article{BatyModelingoxygenuptake2016,
  title = {Modeling the Oxygen Uptake Kinetics during Exercise Testing of Patients with Chronic Obstructive Pulmonary Diseases Using Nonlinear Mixed Models},
  volume = {16},
  issn = {1471-2288},
  doi = {10.1186/s12874-016-0173-8},
  abstract = {BackgroundThe six-minute walk test (6MWT) is commonly used to quantify exercise capacity in patients with several cardio-pulmonary diseases. Oxygen uptake (V\textperiodcentered{}V\textperiodcentered$\backslash$dot \{$\backslash$mathrm \{V\}\}O2) kinetics during 6MWT typically follow 3 distinct phases (rest, exercise, recovery) that can be modeled by nonlinear regression. Simultaneous modeling of multiple kinetics requires nonlinear mixed models methodology. To the best of our knowledge, no such curve-fitting approach has been used to analyze multiple V\textperiodcentered{}V\textperiodcentered$\backslash$dot \{$\backslash$mathrm \{V\}\}O2 kinetics in both research and clinical practice so far.MethodsIn the present study, we describe functionality of the R package medrc that extends the framework of the commonly used packages drc and nlme and allows fitting nonlinear mixed effects models for automated nonlinear regression modeling. The methodology was applied to a data set including 6MWT V\textperiodcentered{}V\textperiodcentered$\backslash$dot \{$\backslash$mathrm \{V\}\}O2 kinetics from 61 patients with chronic obstructive pulmonary disease (disease severity stage II to IV). The mixed effects approach was compared to a traditional curve-by-curve approach.ResultsA six-parameter nonlinear regression model was jointly fitted to the set of V\textperiodcentered{}V\textperiodcentered$\backslash$dot \{$\backslash$mathrm \{V\}\}O2 kinetics. Significant differences between disease stages were found regarding steady state V\textperiodcentered{}V\textperiodcentered$\backslash$dot \{$\backslash$mathrm \{V\}\}O2 during exercise, V\textperiodcentered{}V\textperiodcentered$\backslash$dot \{$\backslash$mathrm \{V\}\}O2 level after recovery and V\textperiodcentered{}V\textperiodcentered$\backslash$dot \{$\backslash$mathrm \{V\}\}O2 inflection point in the recovery phase. Estimates obtained by the mixed effects approach showed standard errors that were consistently lower as compared to the curve-by-curve approach.ConclusionsHereby we demonstrate the novelty and usefulness of this methodology in the context of physiological exercise testing.},
  language = {en},
  number = {1},
  journal = {BMC Medical Research Methodology},
  author = {Baty, Florent and Ritz, Christian and van Gestel, Arnoldus and Brutsche, Martin and Gerhard, Daniel},
  month = dec,
  year = {2016},
  pages = {66},
  file = {/Users/andrea/Documents/_DBXAndrea/Zotero/storage/KA4EZMDE/Baty2016_Modeling_the_oxygen_uptake_kinetics.pdf;/Users/andrea/Documents/_DBXAndrea/Zotero/storage/TPT9UMEU/12874_2016_173_MOESM1_ESM.rda;/Users/andrea/Documents/_DBXAndrea/Zotero/storage/WYK4VAL3/12874_2016_173_MOESM3_ESM.pdf;/Users/andrea/Documents/_DBXAndrea/Zotero/storage/WZXVT8T5/12874_2016_173_MOESM2_ESM.r;/Users/andrea/Documents/_DBXAndrea/Zotero/storage/Z4EJ92Y5/12874_2016_173_MOESM4_ESM.eps.pdf}
