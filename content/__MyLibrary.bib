  month = sep,
  year = {2000},
  keywords = {EL-FAMEs,Microbial community composition,Mycorrhizae,PL-FAMEs,Tillage},
  pages = {1419--1430},
  file = {/Users/andrea/Documents/_DBXAndrea/Zotero/storage/4DG2VUIA/Drijber2000_Changes_in_soil.pdf}
