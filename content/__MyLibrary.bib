  title = {Jackknife {{Inference}} for {{Heteroscedastic Linear Regression Models}}},
  volume = {21},
  copyright = {Copyright \textcopyright{} 1993 Statistical Society of Canada},
  issn = {03195724},
  abstract = {Inference on the regression parameters in a heteroscedastic linear regression model with replication is considered, using either the ordinary least-squares (OLS) or the weighted least-squares (WLS) estimator. A delete-group jackknife method is shown to produce consistent variance estimators irrespective of within-group correlations, unlike the delete-one jackknife variance estimators or those based on the customary ?-method assuming within-group independence. Finite-sample properties of the delete-group variance estimators and associated confidence intervals are also studied through simulation. /// Nous consid\'erons l'inf\'erence pour les param\`etres de r\'egression d'un mod\`ele de r\'egression lin\'eaire h\'et\'erosc\'edastique avec r\'ep\'etition, soit \`a l'aide de l'estimateur par les moindres carr\'es ordinaire (OLS) ou de l'estimateur par les moindres carr\'es pond\'er\'e (WLS). Nous montrons qu'une m\'ethode "delete-group jackknife" produit des estimateurs de la variance convergents, quelles que soient les corr\'elations \`a l'int\'erieur des groupes. Ceci est contraire aux estimateurs de la variance de type "delete-one jackknife" et \`a ceux fond\'es sur la m\'ethode ? usuelle sous l'hypoth\`ese d'ind\'ependance \`a l'int\'erieur des groupes. Nous \'etudions \'egalement des propri\'et\'es d'\'echantillons finis des estimateurs de la variance de type "delete-group" ainsi que des intervalles de confiance associ\'es, \`a l'aide d'une \'etude de simulation.},
  language = {English},
  number = {4},
  journal = {The Canadian Journal of Statistics / La Revue Canadienne de Statistique},
  author = {Shao, Jun and Rao, J. N. K.},
  year = {1993},
  pages = {pp. 377-395},
  file = {/Users/andrea/Documents/_DBXAndrea/Zotero/storage/AIT6CKHU/Shao1993_Jackknife_Inference.pdf}
}

