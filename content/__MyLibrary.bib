@article{barberi2002_Weedmanagementorganic,
  title = {Weed Management in Organic Agriculture: Are We Addressing the Right Isssues?},
  abstract = {Use of organic fertilisers/amendments generally results in slower release of nutrients as compared to mineral fertilisation. Organic fertiliser/amendments may also represent a source of weed seeds, e.g.
