@article{cavero1998_Applicationepicmodel,
  title = {Application of Epic Model to Nitrogen Cycling in Irrigated Processing Tomatoes under Different Management Systems},
  volume = {56},
  abstract = {Vegetable crops such as processing tomatoes (Lycopersicon esculentum Mill.) are usually complex in terms of nitrogen (N) dynamics because of the large amounts absorbed by the crop, the short growing season and the use of irrigation. Complexity increases when N is supplied from an organic source. A crop simulation model could be very useful to improve N management in this crop. Processing tomatoes were grown on raised beds and furrow irrigated in 1994 and 1995 in the Sacramento Valley of California. Fertilizer N and/or purple vetch (Vicia sativa L.) as green manure and composted turkey manure were used as sources of N. The Erosion Productivity Impact Calculator (EPIC) model was calibrated with 1994 data and validated with 1995 data. Plant growth was accurately simulated in the conventional systems that used fertilizer N and in the low input system that used fertilizer N plus vetch. The model accurately simulated above-ground biomass in a system that used vetch and no synthetic fertilizer N, but it over-predicted Leaf Area Index (LAI). Nitrogen deficiency was observed in the plants in this system. The model simulated nitrogen deficiency mainly as a reduction in biomass production but in the real world the reduction of leaf area was the first effect of nitrogen deficiency in the vegetative phase. Yields were accurately predicted except when diseases affected plant growth. A simple reduction factor of nitrate movement in the bed adequately addressed the movement of nitrate. In general, the model accurately predicted the evolution of inorganic nitrogen in different soil layers during the crop season. However, simulated inorganic N in the upper 15 cm was underestimated in the last part of the crop season and consequently N uptake at harvest was slightly over-predicted in some cases. Nitrogen distribution and access of the roots to inorganic nitrogen are discussed as causes of this discrepancy between model simulated and observed values.},
  number = {4},
  journal = {Agricultural Systems},
  author = {Cavero, J. and Plant, R. E. and Shennan, G. and Williams, J. R. and Kiniry, J. R. and Benson, V. W.},
  year = {1998},
  pages = {391--414},
  file = {/Users/andrea/Documents/_DBXAndrea/Zotero/storage/4ETH99CU/Cavero1998_Application_of_epic.pdf}
}

@article{confalonieri2004_preliminaryevaluationsimulation,
  title = {A Preliminary Evaluation of the Simulation Model {{CropSyst}} for Alfalfa},
  volume = {21},
  issn = {1161-0301},
  abstract = {This work stems from the need to set-up appropriate simulation models for scenario analysis of intensive forage cropping systems in northern Italy, where alfalfa plays a major role. CropSyst is a deterministic, process-based, with daily time-step cropping systems simulation model. It can simulate crop growth and development, water and nitrogen balance for herbaceous annual and perennial crops. In this work, it was used to simulate aboveground biomass (AGB) accumulation and soil water content (SWC) for two alfalfa meadows seeded in 1996 and 1997 in Lodi, northern Italy (45\~A,\^A$^\circ$N latitude). The crop was parameterised with data from the literature, local experience and calibration with measured data from the first 2 years. Data from the third year were used for validation.The cumulative yields of the 3-year periods were 38.2 and 36.9tAGBha\~A\textcent\textasciicircum{}'1, obtained with a total of 14 cuts. The set of crop parameters is consistent with values reported in the literature. For most of the cuts, the model simulates appropriately the growth of the crop: the relative root mean squared error (RRMSE) between observed and measured AGB ranged between 3 and 6\% after calibration and between 3 and 5\% after validation. RRMSE for SWC ranged between 13 and 21\% after calibration and between 10 and 20\% after validation. Even if some limitations are explicitly addressed, this crop parameter set can be already used for explorative scenario simulations in the study area. This work has demonstrated the robustness of the model for perennial forage crops simulations and has suggested some improvements of the model (automatic scheduling of cuts, role of crown reserves).},
  number = {2},
  journal = {European Journal of Agronomy},
  author = {Confalonieri, R. and Bechini, L.},
  year = {2004},
  keywords = {Alfalfa,Forage crops,Growth and development,Northern Italy,Simulation model,Soil water content},
  pages = {223--237},
  file = {/Users/andrea/Documents/_DBXAndrea/Zotero/storage/FKN2DQ52/Confalonieri2004_A_preliminary.pdf}
}

