@article{knapp2012_PresentFutureRoles,
  title = {Past, {{Present}}, and {{Future Roles}} of {{Long}}-{{Term Experiments}} in the {{LTER Network}}},
  volume = {62},
  issn = {0006-3568, 1525-3244},
  doi = {10.1525/bio.2012.62.4.9},
  abstract = {The US National Science Foundation\textendash{}funded Long Term Ecological Research (LTER) Network supports a large (around 240) and diverse portfolio of long-term ecological experiments. Collectively, these long-term experiments have (a) provided unique insights into ecological patterns and processes, although such insight often became apparent only after many years of study; (b) influenced management and policy decisions; and (c) evolved into research platforms supporting studies and involving investigators who were not part of the original design. Furthermore, this suite of long-term experiments addresses, at the site level, all of the US National Research Council's Grand Challenges in Environmental Sciences. Despite these contributions, we argue that the scale and scope of global environmental change requires a more-coordinated multisite approach to long-term experiments. Ideally, such an approach would include a network of spatially extensive multifactor experiments, designed in collaboration with ecological modelers that would build on and extend the unique context provided by the LTER Network.},
  language = {en},
  number = {4},
  journal = {BioScience},
  author = {Knapp, Alan K. and Smith, Melinda D. and Hobbie, Sarah E. and Collins, Scott L. and Fahey, Timothy J. and Hansen, Gretchen J. A. and Landis, Douglas A. and Pierre, Kimberly J. La and Melillo, Jerry M. and Seastedt, Timothy R. and Shaver, Gaius R. and Webster, Jackson R.},
  month = jan,
  year = {2012},
  keywords = {climate change,global change,long-term research,LTER Network,multifactor experiments},
  pages = {377-389},
