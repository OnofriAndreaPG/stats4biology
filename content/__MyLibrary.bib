  author = {Bocchini, Marika and D'Amato, Roberto and Ciancaleoni, Simona and Fontanella, Maria C. and Palmerini, Carlo A. and Beone, Gian M. and Onofri, Andrea and Negri, Valeria and Marconi, Gianpiero and Albertini, Emidio and Businelli, Daniela},
