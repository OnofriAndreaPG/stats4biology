@article{rondoni2012_Differentialsusceptibilityspecialized,
  title = {Differential Susceptibility in a Specialized Aphidophagous Ladybird, {{Platynaspis}} Luteorubra ({{Coleoptera}}: {{Coccinellidae}}), Facing Intraguild Predation by Exotic and Native Generalist Predators},
  volume = {22},
  abstract = {Abstract The establishment of exotic generalist predators raises concerns about possible domination of the invaded community and displacement of native species. In this respect, recent studies suggest that specialists are likely to be more at risk of niche displacement than generalists, thereby compromising biological control in agroecosystems. Under laboratory conditions, we examined the susceptibility of a specialized myrmecophilous ladybird, Platynaspis luteorubra Goeze, as intraguild (IG) prey of generalist native (Oenopia conglobata L., Adalia bipunctata L.) and exotic (Harmonia axyridis Pallas) coccinellid species. We described parameters of predatory behaviour (e.g., attack rate and effectiveness of antipredator defence) and their variation among predators. Furthermore, we estimated the effect of these parameters on the time to predation using a Cox proportional hazards model. We found that A. bipunctata was the most effective IG predator of P. luteorubra, with the highest rate of successful attacks; however, P. luteorubra was not very susceptibile to attack by O. conglobata or H. axyridis, and these species exhibited lower rates of successful attack and predation. Our results, together with field observations, suggest that intraguild predation (IGP) is a factor that fosters niche specialization in P. luteorubra and decreases the realized niche of this species in central Italy. Furthermore the lower susceptibility of P. luteorubra to H. axyridis may be due to a lack of historical co-occurrence between preimmaginal stages, possibly arising from differental selection of oviposition sites.},
  number = {11},
  journal = {Biocontrol Science and Technology},
  author = {Rondoni, Gabriele and Onofri, Andrea and Ricci, Carlo},
  year = {2012},
  pages = {1334-1350},
  file = {/Users/andrea/Documents/_DBXAndrea/OneDrive/Bibliografia/_personali/Rondoni2012_Differential_susceptibility_in_a_specialized.pdf}
}

