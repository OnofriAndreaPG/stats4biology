@article{dahal1990_EffectsPrimingEndosperm,
  title = {Effects of {{Priming}} and {{Endosperm Integrity}} on {{Seed Germination Rates}} of {{Tomato Genotypes}}: {{II}}. {{Germination}} at Reduced Water Potential},
  volume = {41},
  abstract = {Seed germination rates (GR =inverse of time to germination) are sensitive to genetic, environmental, and physiological factors. We have compared the GR of tomato (Lycopersicon esculentum Mill.) seeds of cultivar T5 to those of rapidly germinating L. esculentum genotypes PI 341988 and PI 120256 over a range of water potential (psi). The influence of seed priming treatments and removal of the endosperm/testa cap enclosing the radicle tip on germination at reduced psi were also assessed. Germination time-courses at different psi's were analysed according to a model that identified a base, or minimum, psi allowing germination of a specific percentage (g) of the seed population (psib(g)), and a hydrotime constant' (thetaH) indicating the rate of progress toward germination per MPa.h. The distribution of psib(g) determined by probit analysis was characterized by a mean base psi (b) and the standard deviation in psib among seeds (sigmapsib). The three derived parameters, b, sigmapsib) and thetaH, were sufficient to predict the time-courses of germination of intact seeds at any psi. A normalized time-scale for comparing germination responses to reduced psi is introduced. The time to germination at any psi(tg(psi)) can be normalized to be equivalent to that observed in water (tg(0)) according to the equation tg(0)=[l-(psi/psib(g))]tg(psi). PI 341988 seeds were more tolerant of reduced psi and had a more rapid GR than T5 seeds due to both a lower b and a smaller thetaH. The rapid germination of PI 120256, on the other hand, could be attributed entirely to a smaller thetaH. Seed priming (6 d in -1.2 MPa polyethylene glycol 8000 solution at 20 \^A$^\circ$C followed by drying) increased GR at all psi$\backslash$textgreaterpsib(g), but did not lower the minimum psi allowing germination; i.e. priming reduced thetaH without lowering psib. Removing the endosperm/testa cap (cut seeds) markedly increased GR and lowered the mean psi required to inhibit germination by 0.7 to 0.9 MPa. However, this resulted primarily from downward adjustment in b during the incubation of cut seeds at low psi in the test solutions. The difference in b between intact and cut seeds incubated at high psi was much less (0.l MPa), indicating that at the time of radicle protrusion, the endosperm had weakened to the point where it constituted only a small mechanical barrier. In the intact seed, endosperm weakening and the downward adjustment in embryo b ceased at psi $\backslash$textless -0.6 MPa, while the reduction in thetaH associated with priming proceeded down to at least -1.2 MPa. Based on these data and on the pressure required to push the embryos from the seeds at various times after imbibition, it appears that the primary effect of priming was to shorten the time required for final endosperm weakening to occur. However, as priming increased GR even in cut seeds, priming effects on the embryo may control the rate of endosperm weakening.},
  number = {11},
  journal = {J. Exp. Bot.},
  author = {Dahal, Peetembar and Bradford, K. J.},
  year = {1990},
  pages = {1441--1453},
  file = {/Users/andrea/Documents/_DBXAndrea/OneDrive/Bibliografia/SeedGermination/Dahal1990_Effects_of_Priming_and_Endosperm_Integrity_on2.pdf}
}

@article{dahal1990_EffectsPrimingEndosperma,
  title = {Effects of {{Priming}} and {{Endosperm Integrity}} on {{Seed Germination Rates}} of {{Tomato Genotypes}}: {{I}}. {{Germination}} at Suboptimal Temperatures},
  volume = {41},
  abstract = {The bases of differences in germination rates (GRg = inverse of time to germination [tg] of percentage g) among three cold/salt-tolerant tomato (Lycopersicon esculentum Mill.) accessions (PI 341988, PI 120256, and PI 174263) and one cold/salt-sensitive tomato cultivar (T5) were investigated. The effects of seed priming (6 d imbibition in aerated -1.2 MPa polyethylene glycol solution at 20 \^A$^\circ$C followed by redrying) and of removing the endosperm/testa cap covering the radicle on the temperature sensitivity of GRg, and the interaction of these treatments with genotypes, were also examined. GRg decreased linearly with decreasing temperature for all genotypes and seed treatments. The minimum or base temperatures for germination (Tb) varied by [\^a\textperthousand\textcurrency{}] 1 \^A$^\circ$C among the tomato lines, so genotypic differences in GRg were due to differing thermal time requirements for germination. The mean thermal time requirement [IMG]f1.gif" BORDER="0"$\backslash$textgreater for germination of T5 seeds was 22\% and 19\% greater than that of PI 341988 and PI 120256 seeds, respectively, but only 9\% greater than that of PI 174263 seeds. Seed priming did not lower Tb of any genotype, but significantly reduced [IMG]f2.gif" BORDER="0"$\backslash$textgreater by 24, 49, 41, and 49\% in T5, PI 341988, PI 120256, and PI 174263, respectively, indicating that priming increased the rate at which the seeds progressed towards germination when T$\backslash$textgreaterTb, but did not lower the minimum temperature at which germination could occur. Priming increased the GRg of T5 seeds to equal or exceed those of control (non-primed) seeds of the cold/salt-tolerant genotypes at any T$\backslash$textgreaterTb, but the PI lines exhibited an even greater response to priming. Times to germination within each seed lot were normally distributed on a logarithmic scale. Priming increased the variance in tg within a seed lot when compared to control seeds. However, the variation in thermal time for germination between the 10th and 90th percentiles of the seed population (thetaT(10-90)) was relatively unaffected by priming due to the reduction in [IMG]f2.gif" BORDER="0"$\backslash$textgreater in primed seeds. Removing the endosperm cap and testa opposite the radicle tip decreased [IMG]f2.gif" BORDER="0"$\backslash$textgreater almost 6-fold and and reduced Tb by 5 \^A$^\circ$C in T5 and PI 341988, implicating processes in the endosperm/testa as the limiting factors in germination at suboptimal temperatures.},
  number = {11},
  journal = {J. Exp. Bot.},
  author = {Dahal, Peetembar and Bradford, K. J. and Jones, R. A.},
  month = nov,
  year = {1990},
  pages = {1431--1439},
  file = {/Users/andrea/Documents/_DBXAndrea/OneDrive/Bibliografia/SeedGermination/Dahal1990_Effects_of_Priming_and_Endosperm_Integrity_on.pdf}
}

