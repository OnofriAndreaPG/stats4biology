@article{mantel1977_MantelHaenszelAnalysesLittermatched,
  title = {Mantel-{{Haenszel Analyses}} of {{Litter}}-Matched {{Time}}-to-{{Response Data}}, with {{Modifications}} for {{Recovery}} of {{Interlitter Information}}},
