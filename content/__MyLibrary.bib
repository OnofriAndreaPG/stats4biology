@article{dalal1998_SustainingproductivityVertisol,
  title = {Sustaining Productivity of a {{Vertisol}} at {{Warra}}, {{Queensland}}, with Fertilisers, No-Tillage, or Legumes. 5. {{Wheat}} Yields, Nitrogen Benefits and Water-Use Efficiency of Chickpea-Wheat Rotation},
  volume = {38},
  abstract = {\textbf{Summary.} In this study, the benefits of chickpea\&\#8211;wheat rotation compared with continuous wheat cropping (wheat\&\#8211;wheat rotation) were evaluated for their effects on soil nitrate nitrogen, wheat grain yields and grain protein concentrations, and water-use efficiency at Warra, southern Queensland from 1988 to 1996. $<$p$>$Benefits in terms of wheat grain yields varied, from 17\% in 1993 to 61\% in 1990, with a mean increase in grain yield of 40\% (825 kg/ha). Wheat grain protein concentration increased from 9.4\% in a wheat\&\#8211;wheat rotation to 10.7\% in a chickpea\&\#8211;wheat rotation, almost a 14\% increase in grain protein. There was a mean increase in soil nitrate nitrogen of 35 kg N/ha.1.2 m after 6 months of fallow following chickpea (85 kg N/ha) compared with continuous wheat cropping (50 kg N/ha). This was reflected in additional nitrogen in the wheat grain (20 kg N/ha) and above-ground plant biomass (25 kg N/ha) following chickpea.$<$/p$>$ $<$p$>$Water-use efficiency by wheat increased from a mean value of 9.2 kg grain/ha. mm in a wheat\&\#8211;wheat rotation to 11.7 kg grain/ha.mm in a chickpea\&\#8211;wheat rotation. The water-use efficiency values were closely correlated with presowing nitrate nitrogen, and showed no marked distinction between the 2 cropping sequences. Although presowing available water in soil in May was similar in both the chickpea\&\#8211;wheat rotation and the wheat\&\#8211;wheat rotation in all years except 1996, wheat in the former used about 20 mm additional water and enhanced water-use efficiency. Thus, by improving soil fertility through restorative practices such as incorporating chickpea in rotation, water-use efficiency can be enhanced and consequently water runoff losses reduced.$<$/p$>$ $<$p$>$Furthermore, beneficial effects of chickpea in rotation with cereals could be enhanced by early to mid sowing (May\&\#8211;mid June) of chickpea, accompanied by zero tillage practice. Wheat of \&\#8216;Prime Hard\&\#8217; grade protein (\&\#x2265;13\%) could be obtained in chickpea\&\#8211;wheat rotation by supplementary application of fertiliser N to wheat.$<$/p$>$ $<$p$>$In this study, incidence of crown rot of wheat caused by \emph{Fusarium graminearum} was negligible, and incidence and severity of common root rot of wheat caused by \emph{Bipolaris sorokiniana} were essentially similar in both cropping sequences and inversely related to the available water in soil at sowing. No other soil-borne disease was observed. Therefore, beneficial effects of chickpea on wheat yields and grain protein were primarily due to additional nitrate nitrogen following the legume crop and consequently better water-use efficiency.$<$/p$>$},
  number = {5},
  journal = {Australian Journal of Experimental Agriculture},
  author = {Dalal, R. C. and Strong, W. M. and Weston, E. J. and Cooper, J. E. and Wildermuth, G. B. and Lehane, K. J. and King, A. J. and Holmes, C. J.},
  month = jan,
  year = {1998},
  pages = {489-501}
}

