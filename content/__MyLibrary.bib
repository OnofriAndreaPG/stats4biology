@article{hardegree2006_PredictingGerminationResponse,
  title = {Predicting {{Germination Response}} to {{Temperature}}. {{III}}. {{Model Validation Under Field}}-Variable {{Temperature Conditions}}},
  volume = {98},
  doi = {10.1093/aob/mcl163},
  abstract = {\textbullet{}Background and Aims Two previous papers in this series evaluated model fit of eight thermal-germination models parameterized from constant-temperature germination data. The previous studies determined that model formulations with the fewest shape assumptions provided the best estimates of both germination rate and germination time. The purpose of this latest study was to evaluate the accuracy and efficiency of these same models in predicting germination time and relative seedlot performance under field-variable temperature scenarios.\textbullet{}Methods The seeds of four rangeland grass species were germinated under 104 variable-temperature treatments simulating six planting dates at three field sites in south-western Idaho. Measured and estimated germination times for all subpopulations were compared for all models, species and temperature treatments.\textbullet{}Key Results All models showed similar, and relatively high, predictive accuracy for field-temperature simulations except for the iterative-probit-optimization (IPO) model, which exhibited systematic errors as a function of subpopulation. Highest efficiency was obtained with the statistical-gridding (SG) model, which could be directly parameterized by measured subpopulation rate data. Relative seedlot response predicted by thermal time coefficients was somewhat different from that estimated from mean field-variable temperature response as a function of subpopulation.\textbullet{}Conclusions All germination response models tested performed relatively well in estimating field-variable temperature response. IPO caused systematic errors in predictions of germination time, and may have degraded the physiological relevance of resultant cardinal-temperature parameters. Comparative indices based on expected field performance may be more ecologically relevant than indices derived from a broader range of potential thermal conditions.},
  number = {4},
  journal = {Annals of Botany},
  author = {Hardegree, Stuart P.},
  year = {2006},
  pages = {827-834},
  file = {/Users/andrea/Documents/_DBXAndrea/OneDrive/Bibliografia/SeedGermination/Hardegree2006_Predicting_Germination_Response_to_Temperature2.pdf}
}

